.mui-object-pill {
    padding: 3px 4px;
    background-color: var(--muse-chat-backgrounds-100);
    display: flex;
    flex-direction: row;
    border-radius: 3px;
    justify-content: center;
    align-items: center;
}

.mui-object-pill-icon {
    height: 12px;
    width: 12px;
    margin-right: 2px;
    display: flex;
    justify-content: center;
    align-items: center;
}

.mui-execution-pill-container {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}
